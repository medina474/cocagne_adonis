@layout.main()

  <h1>Calendriers</h1>

 <table>
  @each(calendrier in calendriers)
    <li><a href="{{ route('admin_calendriers.show', { id: calendrier.id }) }}">{{ calendrier.calendrier }}</a>
    <form method="POST" action="{{ route('admin_calendriers.destroy', { id: calendrier.id }) }}?_method=DELETE">
     {{ csrfField() }}
  <button type="submit">Supprimer</button>
</form>
    </li>
  @endeach
 </table>
@end
